{
  "name": "all-new-tsbp",
  "scripts": {
    "build": "swc ./src -d ./dist/build/src --config-file .swcrc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "start": "node ./dist/build/src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "js-yaml": "^4.1.0",
    "libsodium-wrappers": "^0.7.11",
    "node-fetch": "^3.3.1",
    "play-dl": "^1.9.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.2.5",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}