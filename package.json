{
  "name": "all-new-tsbp",
  "scripts": {
    "build": "swc ./src -d ./dist/build/src --config-file .swcrc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "start": "node ./dist/build/src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "libsodium-wrappers": "^0.7.11",
    "node-fetch": "^3.3.1",
    "play-dl": "^1.9.6",
    "youtube-dl-exec": "^2.4.0",
    "ytdl-core": "^4.11.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.41",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
